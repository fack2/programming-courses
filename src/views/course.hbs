<link href="/courseStyle.css" rel="stylesheet" type="text/css">


<ul>
    {{#each courses}}
    <div class="details">
        <li>Course Name: {{this.name}}</li>
        <li>Description: {{this.description}}</li>
        <li>Cost: {{this.costs}}</li>
        <li>Teacher Name: {{this.teachername}}</li>
        <li>Available: {{this.available}}</li>
        <li>Time: {{this.coursetime}}</li>
    </div>
    <div class="imgCourse">
        <img src={{this.img}}>
    </div>

    {{/each}}
</ul>